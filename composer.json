{
    "name": "fdevs/serializer",
    "type": "library",
    "description": "FDevs Serializer Component",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Samusev",
            "email": "andrey_simfi@list.ru",
            "homepage": "http://andrey.4devs.io"
        },
        {
            "name": "4devs Community",
            "homepage": "https://github.com/4devs/serializer/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "psr/cache": "~1.0",
        "psr/log": "^1.0",
        "symfony/config": "~4.0",
        "symfony/options-resolver": "^4.0",
        "symfony/property-access": "~4.0",
        "symfony/serializer": "~4.0"
    },
    "require-dev": {
        "composer/semver": "~1.4",
        "friendsofphp/php-cs-fixer": "^2.10",
        "phpunit/phpunit": "^6.5"
    },
    "suggest": {
        "composer/semver": "use with visible versions"
    },
    "config":{
        "bin-dir": "bin",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "FDevs\\Serializer\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
